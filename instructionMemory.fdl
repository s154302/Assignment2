ipblock instmem(in address : ns(5);
		in wr, rd : ns(1);
		in idata : ns(32);
		out odata : ns(32)){

	iptype "ram";
	ipparm "size=64";
	ipparm "wl=32";
	//ipparm "file=program.txt";
}

dp PC(out pc_out : ns(5)) {
  reg a : ns(5);

  always{
    a = a + 1;
    pc_out = a;
  }

}

system System {
  PC(pc_out);
  instmem(address, wr, rd, idata, odata);
}
